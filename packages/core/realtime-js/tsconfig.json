{
  "extends": "../../../tsconfig.base.json",
  "include": ["src"],
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "module": "CommonJS",
    "outDir": "dist/main",
    "rootDir": "src",
    "sourceMap": true,
    "target": "ES2017",
    "lib": ["ES2017", "DOM"],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "stripInternal": true,
    "isolatedModules": false
  },
  "references": []
}

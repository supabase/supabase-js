    "jest-expo": "~53.0.5",
    "react-test-renderer": "^19.2.0",
    "typescript": "^5.8.3",
    "ws": "^8.18.2"
  },
  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jest.setup.js"
